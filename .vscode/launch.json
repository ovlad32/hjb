  "version": "0.2.0",
  "configurations": [
    {
      "name": "Indexing",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "env": {},
      "args": [
        "-mode",
        "indexing",
        "-baseTag",
        "mt-base"
      ],
      "args2": [
        "-mode",
        "indexing",
        "-baseTag",
        "grp1-base"
      ]
    },
    {
      "name": "Discovery",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "env": {},
      "args": [
        "-mode",
        "discovery",
        "-baseTag=mt-base",
        "-lakeTag=cra-lake",
        "-anchorUniqueness=1",
        "-anchorMinCount=2",
        "-anchorMaxCount=5",
      ],
      "args2": [
        "-mode",
        "discovery",
        "-baseTag",
        "grp1-base",
        "-lakeTag",
        "grp1-lake",
        "-rate",
        "3"
      ]
    },
    {
      "name": "Aggregation",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "env": {},
      "args": [
        "-mode=aggregation",
        "-baseTag=mt-base",
        "-baseRowMinPct=0"
      ]
    }
  ]
}